{
  "name": "arc-treasury",
  "private": true,
  "version": "1.0.0",
  "description": "Premium digital treasury platform for managing stablecoins on Arc Network",
  "author": "Arc Treasury",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/arc-treasury.git"
  },
  "keywords": [
    "defi",
    "treasury",
    "stablecoin",
    "web3",
    "ethereum",
    "arc-network",
    "usdc",
    "eurc"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "compile": "hardhat compile",
    "test": "npm run test:contracts",
    "test:contracts": "cd contracts && npm install && npx hardhat test",
    "deploy:testnet": "hardhat run scripts/deploy.ts --network arcTestnet",
    "deploy:local": "hardhat run scripts/deploy.ts --network hardhat",
    "check:contracts": "hardhat run scripts/checkContracts.ts --network arcTestnet",
    "check:tx": "hardhat run scripts/checkTransaction.ts --network arcTestnet",
    "setup:referral": "node scripts/setup-referral-tables.cjs"
  },
